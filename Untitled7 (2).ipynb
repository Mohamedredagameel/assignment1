    "            count += 1\n",
    "\n",
    "    return count\n",
    "\n",
    "my_list = [12, \"apple\", \"banana\", 45, \"cat\", \"dog\"]\n",
    "string_count = count_strings(my_list)\n",
    "print(\"The number of strings in the list is:\", string_count)"
